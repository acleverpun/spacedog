{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "baseUrl": "./src",
        "rootDir": "./src",
        "outDir": "./Resources",
        "declaration": false,
        "removeComments": false,
        "inlineSourceMap": false,
        "noLib": false,
        "noEmitOnError": true,
        "noImplicitAny": false,
        "allowJs": true,
        "lib": [
            "es5"
        ]
    },
    "include": [
        "src",
        "typings"
    ]
}
